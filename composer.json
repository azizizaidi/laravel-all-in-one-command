{
    "name": "azizizaidi/laravel-all-in-one-command",
    "description": "A Laravel package to generate all necessary files for a feature with a single command.",
    "keywords": ["laravel", "artisan", "command", "generator", "scaffold", "crud", "package"],
    "homepage": "https://github.com/azizizaidi/laravel-all-in-one-command",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Azizi Zaidi",
            "email": "azizikuis@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/azizizaidi/laravel-all-in-one-command/issues",
        "source": "https://github.com/azizizaidi/laravel-all-in-one-command"
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "AziziZaidi\\AllInOneCommand\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AziziZaidi\\AllInOneCommand\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AziziZaidi\\AllInOneCommand\\AllInOneCommandServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}